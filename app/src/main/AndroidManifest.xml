<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="br.com.grupofleury.pagamento_exemplo">
<uses-permission android:name="android.permission.INTERNET" />

  <application
      android:allowBackup="true"
      android:icon="@mipmap/ic_launcher"
      android:label="@string/app_name"
      android:roundIcon="@mipmap/ic_launcher_round"
      android:supportsRtl="true"
      android:theme="@style/AppTheme"
      tools:node="merge">
    <activity android:name=".MainActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />

        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>
    <activity android:name="br.com.grupofleury.pagamento.presentation.view.AddCardActivity" />
    <activity android:name="br.com.grupofleury.pagamento.presentation.view.ScheduleResumeActivity" />
    <activity android:name="br.com.grupofleury.pagamento.presentation.view.ScheduleFinishedActivity" />
    <activity android:name="br.com.grupofleury.pagamento.presentation.view.cardControl.CardControlActivity"/>
  </application>

</manifest>